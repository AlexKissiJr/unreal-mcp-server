{
  "name": "@unreal/mcp-server",
  "version": "1.0.0",
  "description": "MCP Server for Unreal Engine integration",
  "type": "module",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "test": "node src/test-tcp-client.js",
    "dev": "nodemon src/index.js"
  },
  "keywords": [
    "mcp",
    "unreal",
    "server",
    "smithery"
  ],
  "author": "Alex Kissi Jr",
  "license": "MIT",
  "dependencies": {
    "debug": "^4.3.4"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "smithery": {
    "type": "server",
    "transport": {
      "type": "tcp",
      "port": 13378
    },
    "tools": [
      "echo",
      "get_time",
      "unreal_info",
      "server_status"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/alexkissijr/unreal-mcp-server.git"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
